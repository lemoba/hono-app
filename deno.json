{
  "imports": {
    "@/": "./src/",
    "hono": "jsr:@hono/hono@^4.6.12",
    "prisma": "jsr:@prisma/client@^6.0.1"
  },
  "tasks": {
    "start": "denon run -A src/main.ts",
    "prisma-db-pull": "deno run -A npm:prisma@latest db pull",
    "prisma-generate": "deno run -A npm:prisma@latest generate"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}